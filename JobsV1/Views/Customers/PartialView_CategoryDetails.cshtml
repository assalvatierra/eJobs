@model IEnumerable<JobsV1.Models.CustCategory>
    @{
        int customerid = Convert.ToInt32(ViewData["customerId"]);
    }

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td >
            <img src="@Html.DisplayFor(modelItem => item.iconPath)" width="25" height="25" style="margin-right:20px;"/>
           
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Remove", "Remove", "CustCats",new { catid = item.Id, custid = customerid }, null)
        </td>
    </tr>
}

</table>
