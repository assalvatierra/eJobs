@model JobsV1.Models.CarRateGroup

@{
    ViewBag.Title = "Add to Group";
}

<h2>Add to Group</h2>

<div class="col-lg-12">
    <div class="list-group" id="categorylist2">
        @foreach (var packages in ViewBag.CarRatePackageList as List<JobsV1.Models.CarRatePackage>)
        {
            
            @Html.ActionLink(packages.Description, "AddToGroup","RateGroups", new { id = packages.Id, GroupId = ViewBag.RateGroupId }, new { @class = "list-group-item" })
        }
    </div>
</div>

<div>
    @Html.ActionLink("Back to List", "CarRateGroupList", new { rateGroupId = ViewBag.RateGroupId },null)
</div>
