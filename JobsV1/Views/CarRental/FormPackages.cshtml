
    <div class="form-horizontal">
        
        <div class="col-md-8">
            <h4> Packages </h4>
            <table class="table table-bordered">
                <tr>
                    <th>Package</th>
                    <th>Rate </th>
                    <th>Driver Accomodation/Meals</th>
                    <th>Fuel</th>
                    <th>No Days</th>
                    <th>Total</th>
                </tr>

                @foreach (var package in ViewBag.Packages as IEnumerable<JobsV1.Models.CarRatePackage>)
                {

                    <tr>
                        <td>@package.Description</td>
                        <td>- </td>
                        <td>-</td>
                        <td>-</td>
                        <td>-</td>
                        <td>0</td>
                    </tr>

                }
            </table>
        </div>
    </div>